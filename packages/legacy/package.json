{
    "name": "@tonlabs/uikit.legacy",
    "version": "3.0.0",
    "description": "Legacy components of Tonlabs's UIKit",
    "scripts": {
        "prepare": "bob build"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tonlabs/UIKit.git",
        "directory": "packages/legacy"
    },
    "keywords": [
        "tonlabs",
        "design-system",
        "react-native"
    ],
    "author": "tonlabs.io",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/tonlabs/UIKit/issues"
    },
    "homepage": "https://github.com/tonlabs/UIKit/packages/legacy#readme",
    "main": "lib/commonjs/index.js",
    "module": "lib/module/index.js",
    "react-native": "src/index.js",
    "files": [
        "lib/",
        "src/"
    ],
    "dependencies": {
        "@tonlabs/uikit.components": "^3.0.0",
        "@tonlabs/uikit.core": "^3.0.0",
        "@tonlabs/uikit.themes": "^3.0.0"
    },
    "devDependencies": {
        "@babel/runtime": "^7.15.0",
        "@react-native-community/bob": "0.16.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.65.1",
        "react-native-fast-image": "^8.5.11",
        "react-native-svg": "^12.1.0"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-native": "^0.65.0",
        "react-native-fast-image": "^8.5.11",
        "react-native-svg": "^12.1.0"
    },
    "@react-native-community/bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "configFile": "../../babel.config.js",
                    "copyFlow": true
                }
            ],
            [
                "module",
                {
                    "configFile": "../../babel.config.js",
                    "copyFlow": true
                }
            ]
        ]
    }
}
