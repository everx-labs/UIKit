{
    "name": "@tonlabs/uikit.assets",
    "version": "2.3.0",
    "description": "Assets for Tonlabs's UIKit",
    "scripts": {
        "prepare": "bob build"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tonlabs/UIKit.git",
        "directory": "packages/assets"
    },
    "keywords": [
        "tonlabs",
        "design-system",
        "react-native"
    ],
    "author": "tonlabs.io",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/tonlabs/UIKit/issues"
    },
    "homepage": "https://github.com/tonlabs/UIKit/packages/assets#readme",
    "main": "commonjs/index.js",
    "module": "module/index.js",
    "react-native": "src/index.ts",
    "types": "typescript/src/index.d.ts",
    "files": [
        "commonjs/",
        "module/",
        "typescript/",
        "src/",
        "404/",
        "bank-cards/",
        "brand/",
        "btn_close/",
        "btn_dots/",
        "btn_minimize/",
        "btn_msg_send/",
        "btn_plus/",
        "btn_plus_disabled/",
        "btn_sticker_disabled/",
        "btn_sticker_enabled/",
        "countries/",
        "face-id/",
        "ico-arrow-left/",
        "ico-arrow-right/",
        "ico-book/",
        "ico-camera/",
        "ico-checkbox-circle-active/",
        "ico-checkbox-circle-inactive/",
        "ico-checkbox-square-active/",
        "ico-checkbox-square-inactive/",
        "ico-clear/",
        "ico-close/",
        "ico-cloud-black/",
        "ico-cloud-white/",
        "ico-delete/",
        "ico-doc-blue/",
        "ico-doc-white/",
        "ico-file-income-blue/",
        "ico-file-income-white/",
        "ico-glass/",
        "ico-menu-contained/",
        "ico-open-menu/",
        "ico-progress/",
        "ico-radiobutton-active/",
        "ico-radiobutton-inactive/",
        "ico-scan/",
        "ico-search/",
        "ico-toggle-active/",
        "ico-toggle-inactive/",
        "ico-toggle-off/",
        "ico-toggle-on/",
        "ico-touch-id/",
        "ico-triangle/",
        "ico-unfold/",
        "icon-plus/",
        "icon-plus-disabled/",
        "key-thin/",
        "logo/"
    ],
    "devDependencies": {
        "@react-native-community/bob": "0.16.2"
    },
    "@react-native-community/bob": {
        "source": "src",
        "output": ".",
        "targets": [
            [
                "commonjs",
                {
                    "configFile": "../../babel.config.js",
                    "copyFlow": true
                }
            ],
            [
                "module",
                {
                    "configFile": "../../babel.config.js",
                    "copyFlow": true
                }
            ],
            [
                "typescript",
                {
                    "project": "tsconfig.build.json"
                }
            ]
        ]
    }
}
