{
    "name": "@tonlabs/uistory.browser",
    "version": "2.3.0",
    "description": "Browser based on Tonlabs's UIKit",
    "scripts": {
        "tsc": "tsc --noEmit --composite false",
        "prepare": "bob build"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tonlabs/UIKit.git",
        "directory": "stories/browser"
    },
    "keywords": [
        "tonlabs",
        "react-native",
        "browser"
    ],
    "author": "tonlabs.io",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/tonlabs/UIKit/issues"
    },
    "homepage": "https://github.com/tonlabs/UIKit/stories/browser#readme",
    "main": "lib/commonjs/index.js",
    "module": "lib/module/index.js",
    "react-native": "src/index.ts",
    "types": "lib/typescript/src/index.d.ts",
    "files": [
        "lib/",
        "src/"
    ],
    "dependencies": {
        "@tonlabs/uikit.assets": "^2.3.0",
        "@tonlabs/uistory.chats": "^2.3.0",
        "@tonlabs/uicast.country-picker": "^2.3.0",
        "@tonlabs/uicast.date-time-picker": "^2.3.0",
        "@tonlabs/uicast.qr-code-scanner-sheet": "^2.3.0",
        "@tonlabs/uicast.keyboard": "^3.0.0",
        "@tonlabs/uikit.core": "^2.3.0",
        "@tonlabs/uikit.themes": "^2.3.0",
        "@tonlabs/uikit.controls": "^2.3.0",
        "@tonlabs/uikit.media": "^2.3.0",
        "@tonlabs/uikit.layout": "^2.3.0",
        "@tonlabs/uikit.inputs": "^2.3.0",
        "@tonlabs/uikit.scrolls": "^2.3.0",
        "@tonlabs/uikit.popups": "^2.3.0",
        "@tonlabs/localization": "^2.3.0",
        "bignumber.js": "9.0.0",
        "react-native-safe-area-context": "^3.1.3"
    },
    "devDependencies": {
        "@react-native-community/bob": "0.16.2",
        "@types/react": "17.0.21",
        "@types/react-native": "0.65.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.65.1"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-native": "^0.65.0"
    },
    "@react-native-community/bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "configFile": "./babel.config.js"
                }
            ],
            [
                "module",
                {
                    "configFile": "./babel.config.js"
                }
            ],
            [
                "typescript",
                {
                    "project": "tsconfig.build.json"
                }
            ]
        ]
    }
}
